{
  "name": "wetube",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "nodemonConfig": {
    "ext": "js json mjs pug",
    "delay": "2000"
  },
  "scripts": {
    "dev:server": "nodemon --exec babel-node src/init.js --ignore 'scss'",
    "dev:assets": "cd src && cross-env WEBPACK_ENV=development webpack -w --config webpack.config.js",
    "bulid:server": "babel src --out-dir build --ignore 'src/assets' --ignore'src/static' --ignore 'src/webpack.config.js'",
    "build:assets": "cd src && cross-env WEBPACK_ENV=production webpack --config webpack.config.js",
    "copyAll": "cp -R src/static build && cp -R src/views build",
    "tunnel": "lt --port 5000",
    "build": "npm run bulid:server && npm run build:assets && npm run copyAll",
    "prebuild": "rm -rf build",
    "start": "PRODUCTION=true node build/init.js",
    "prestart": "npm run build"
  },
  "engines": {
    "node": "0.8",
    "npm": "1.2.x"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/polyfill": "^7.12.1",
    "autoprefixer": "^9.4.2",
    "aws-sdk": "^2.818.0",
    "axios": "^0.21.0",
    "babel-loader": "^8.2.1",
    "body-parser": "^1.19.0",
    "connect-mongo": "^3.2.0",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "get-blob-duration": "^1.1.2",
    "helmet": "^3.18.0",
    "mongoose": "^5.10.15",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "ngrok": "^3.3.0",
    "node-sass": "^4.11.0",
    "passport": "^0.4.1",
    "passport-facebook": "^3.0.0",
    "passport-github": "^1.1.0",
    "passport-kakao": "^1.0.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^6.0.1",
    "passport-naver": "^1.0.6",
    "prettier-plugin-pug": "^1.0.0-alpha.8",
    "pug": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/node": "^7.10.5",
    "@babel/preset-env": "^7.11.5",
    "cross-env": "^7.0.3",
    "css-loader": "^2.0.1",
    "eslint": "^5.10.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^3.0.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "nodemon": "^2.0.4",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.15.3",
    "sass-loader": "^7.1.0",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2"
  }
}
